num=int(input())
if num==1:
    print(1)
elif num==2:
    print(22)
    print(22)
else:
    for i in range(0,num):

        if(i==0 or i==num-1):
            for j in range(0,num):
                print(num,end="")
            print()

        else:
            print(num,end="")
            for j in range(0,num-2):
                print(" ",end="")
            print(num)

    

